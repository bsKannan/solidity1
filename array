//odd or even in array
pragma solidity ^0.4.0;
contract name
{
    uint256[] a;
    function get(uint256 n) public view returns(uint256[])
    {
        for(uint256 i=0;i<=n;i++)
       {
                   if(i%2==0)
                   a.push(i);
       }
      for(uint256 j=n;n>=j;j--)
        {
            if(j%2!=0)
           a.push(j);
        }
         return (a);
 }
}
